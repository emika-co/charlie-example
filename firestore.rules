rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
    	allow read, write: if false;
    }
    match /banks/{bank} {
    	allow read: if true;
    	allow write: if false;
    }
    match /sellers/{seller} {
    	allow read: if request.auth != null && request.auth.uid == request.resource.data.uid;
      allow write: if false;
    }
    match /stores/{store} {
    	allow read: if true;
      allow write: if false;
    }
  }
}